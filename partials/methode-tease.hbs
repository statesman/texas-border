<div class="sider panel panel-default sider-social">
  <div class="panel-heading">
    <h3 class="panel-title">{{title}}</h3>
  </div>
  <div class="panel-body">
    <a target="_blank" href="{{link}}"><img src="{{imageUrl}}" class="img-responsive" alt="Cemetary behind the border wall"/></a>
    <h4><a target="_blank" href="{{link}}">{{ headline }}</a></h4>
    <p class="post">{{text}}</p>
    <h3>More in this series</h3>
    <ul class="fa-ul">
    {{#each links}}
      <li><i class="fa-li fa fa-angle-right"></i><a target="_blank" href="{{url}}">{{headline}}</a></li>
    {{/each}}
    </ul>
  </div>
</div>
