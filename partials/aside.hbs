<div class="sider panel panel-default sider-social">
  <div class="panel-heading">
    <h3 class="panel-title">{{header}}</h3>
  </div>
  <div class="panel-body">
    <img src="{{url}}" class="img-responsive" alt=""/>
    <p class="post">{{text}}</p>
    <ul class="list-inline text-center">
      <li><button class="btn btn-default" type="button"><a href="{{link}}" target="_blank"><small><i class="fa fa-comments"></i> RSVP</small></a></button></li>
    </ul>
  </div>
</div>
